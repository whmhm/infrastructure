{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "es6"],
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}